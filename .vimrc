set tabstop=4
set expandtab
syntax on
set shiftwidth=4
set softtabstop=4
set autoindent
set smartindent
set termguicolors
colorscheme monokai_pro
set number
nnoremap / /\v
vnoremap / /\v
let mapleader = ","
set ignorecase
set smartcase
set gdefault
set incsearch
set showmatch
set hlsearch 
nnoremap <leader><space> :noh<cr>
nnoremap <Leader>cc :set colorcolumn=80<cr>
nnoremap <Leader>ncc :set colorcolumn-=80<cr>
nnoremap <tab> %
vnoremap <tab> %

